{
    "manifest_version": 2,
    "name": "ShadowTube",
    "version": "1.1",
    "description": "Embrace the darkness on YouTube! This Chrome extension magically transforms light mode into an eye-friendly, captivating dark mode experience.",
    "icons": {
        "16": "./images/16.png",
        "48": "./images/48.png",
        "128": "./images/128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "./images/16.png",
            "48": "./images/48.png",
            "128": "./images/128.png"
        }
    },
    "content_scripts": [{
        "matches": [
            "https://www.youtube.com/*"
        ],
        "css": [
            "./style/css/youtube.css"
        ]
    }]
}